{
  "name": "personal-finance-manager",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:smoke": "playwright test --grep @smoke",
    "test:all": "npm run test:coverage && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "security:audit": "npm audit --audit-level moderate",
    "security:scan": "eslint . --ext .ts,.tsx",
    "performance:analyze": "npm run build && npx webpack-bundle-analyzer dist/assets/*.js",
    "lighthouse": "lhci autorun",
    "ci:test": "npm run type-check && npm run test:coverage && npm run security:scan",
    "ci:e2e": "npm run build && npm run test:e2e"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.0",
    "@sentry/react": "^9.38.0",
    "@sentry/tracing": "^7.120.3",
    "@supabase/supabase-js": "^2.39.0",
    "date-fns": "^3.0.0",
    "lucide-react": "^0.344.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^6.0.0",
    "react-hook-form": "^7.48.0",
    "react-hot-toast": "^2.5.2",
    "recharts": "^2.9.0",
    "winston": "^3.17.0",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@playwright/test": "^1.54.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "autoprefixer": "^10.4.18",
    "eslint": "^9.9.1",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "globals": "^15.9.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "typescript-eslint": "^8.3.0",
    "vite": "^5.4.2",
    "vitest": "^3.2.4"
  }
}
